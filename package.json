{
  "name": "typescript-react-native-web-scaffold",
  "version": "0.1.0",
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "private": true,
  "keywords": [
    "react",
    "react-native",
    "react-native-web",
    "universal-app",
    "typescript"
  ],
  "scripts": {
    "start:native": "react-native-scripts-ts start",
    "start:web": "./node_modules/.bin/webpack-dev-server -d --config ./webpack.config.js --inline --hot --content-base public/ --open",
    "build:android": "exp build:android",
    "build:ios": "exp build:ios",
    "build:web": "NODE_ENV=production ./node_modules/.bin/webpack -p --config ./webpack.config.js",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "~16.3.0",
    "react-dom": "~16.3.0",
    "react-native": "0.55.0",
    "react-native-web": "^0.5.1"
  },
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/react": "~16.3.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-native": "^0.55.0",
    "@types/react-navigation": "^1.2.1",
    "@types/react-test-renderer": "^16.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-expo": "^27.0.1",
    "react-native-maps": "^0.21.0",
    "react-native-scripts-ts": "1.11.3",
    "react-native-typescript-transformer": "^1.2.3",
    "react-scripts": "1.1.4",
    "react-test-renderer": "16.3.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^22.4.5",
    "ts-loader": "^4.0.0",
    "tslib": "^1.7.1",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.0.0",
    "webpack-manifest-plugin": "^2.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "preset": "jest-expo",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "react-native": "<rootDir>/node_modules/react-native-web",
      "src": "<rootDir>/src"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
